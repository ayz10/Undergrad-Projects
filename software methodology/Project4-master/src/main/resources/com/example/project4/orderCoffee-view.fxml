<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.project4.OrderCoffeeController">
   <children>
      <CheckBox fx:id="cream" layoutX="49.0" layoutY="148.0" mnemonicParsing="false" onAction="#calculatePrice" text="Cream" />
      <CheckBox fx:id="milk" layoutX="49.0" layoutY="173.0" mnemonicParsing="false" onAction="#calculatePrice" text="Milk" />
      <CheckBox fx:id="syrup" layoutX="49.0" layoutY="200.0" mnemonicParsing="false" onAction="#calculatePrice" text="Syrup" />
      <CheckBox fx:id="caramel" layoutX="49.0" layoutY="225.0" mnemonicParsing="false" onAction="#calculatePrice" text="Caramel" />
      <CheckBox fx:id="whipped" layoutX="49.0" layoutY="253.0" mnemonicParsing="false" onAction="#calculatePrice" text="Whipped Cream" />
      <TextField fx:id="subtotal" layoutX="268.0" layoutY="262.0" />
      <Button fx:id="submit" layoutX="291.0" layoutY="336.0" mnemonicParsing="false" onAction="#submit" text="Add to Order" />
      <ComboBox fx:id="size" layoutX="291.0" layoutY="132.0" onAction="#calculatePrice" prefWidth="150.0" />
      <ComboBox fx:id="amount" layoutX="291.0" layoutY="196.0" onAction="#calculatePrice" prefWidth="150.0" />
   </children>
</AnchorPane>
