<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".storeOrderActivity">

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="406dp"
            android:layout_height="686dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"
            app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
    >
        <TextView
                android:text="@string/store_orders"
                android:layout_width="281dp"
                android:layout_height="64dp" android:id="@+id/head"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="16dp" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="41dp" android:layout_marginEnd="64dp"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="465dp" android:textSize="34sp"
                app:layout_constraintVertical_bias="0.0"/>
        <ListView
                android:layout_width="355dp"
                android:layout_height="454dp"
                android:layout_marginTop="17dp"
                app:layout_constraintTop_toBottomOf="@+id/head" app:layout_constraintStart_toStartOf="parent"
                android:layout_marginStart="16dp" android:layout_marginEnd="21dp"
                app:layout_constraintEnd_toEndOf="parent" app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="17dp" android:id="@+id/orders"/>
        <TextView
                android:text="@string/click_to_delete_order"
                android:layout_width="275dp"
                android:layout_height="46dp" android:id="@+id/help"
                app:layout_constraintTop_toBottomOf="@+id/head"
                app:layout_constraintBottom_toTopOf="@+id/orders" app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>